  "bugs": {
    "url": "https://github.com/WEACOMRU/react-plain-calendar/issues"
  },
  "homepage": "https://github.com/WEACOMRU/react-plain-calendar#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.2.2",
